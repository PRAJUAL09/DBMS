CREATE TABLE SALESMAN (SALESMAN_ID VARCHAR(20) PRIMARY KEY,
NAME VARCHAR(20),CITY VARCHAR(20),COMMISSION VARCHAR(20)); 
CREATE TABLE CUSTOMER(CUSTOMER_ID VARCHAR(20) PRIMARY KEY ,CUST_NAME CHAR(20),CITY VARCHAR(20),GRADE VARCHAR(20));
CREATE TABLE ORDERS(ORD_NO INTEGER PRIMARY KEY,PURCHASE_AMT INTEGER,ORD_DATE DATE); 
